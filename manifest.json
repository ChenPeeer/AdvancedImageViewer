{
  "manifest_version": 2,
  "name": "__MSG_extension_title__",
  "version": "1.1",
  "description": "__MSG_extension_description__",
  "default_locale": "en",
  "icons": {
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "512": "icons/icon512.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["res/*"]
}